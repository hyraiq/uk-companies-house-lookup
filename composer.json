{
    "name": "hyraiq/uk-companies-house-lookup",
    "description": "UK business number validation and verification using the UK Companies House web services API",
    "type": "library",
    "require": {
        "php": ">=8.0",
        "ext-json": "*",
        "symfony/validator": "^5.0",
        "symfony/http-client": "^5.0",
        "doctrine/annotations": "^1.0",
        "phpdocumentor/reflection-docblock": "^5.3",
        "phpstan/phpdoc-parser": "^1.2",
        "symfony/property-access": "^5.0",
        "symfony/property-info": "^5.0",
        "symfony/serializer": "^5.0",
        "symfony/cache": "^5.0"
    },
    "require-dev": {
        "ext-zip": "*",
        "phpunit/phpunit": "^9.5",
        "friendsofphp/php-cs-fixer": "^3.3",
        "vimeo/psalm": "^4.14",
        "phpstan/phpstan": "^1.2",
        "phpmd/phpmd": "^2.11",
        "fakerphp/faker": "1.14.1",
        "symfony/flex": "^2.0",
        "phpstan/phpstan-strict-rules": "^1.1",
        "phpstan/phpstan-phpunit": "^1.0",
        "pepakriz/phpstan-exception-rules": "^0.12.0",
        "staabm/annotate-pull-request-from-checkstyle": "^1.7",
        "mheap/phpunit-github-actions-printer": "^1.5"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Hyra\\UkCompaniesHouseLookup\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hyra\\Tests\\UkCompaniesHouseLookup\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "^5.0"
        }
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true
        }
    }
}
